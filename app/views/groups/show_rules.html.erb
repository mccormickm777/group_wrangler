<% provide(:title, @title) %>
<h1><%= @title %></h1>

<div class="row">
  <div class="span2"><h4>Name</h4></div>
  <div class="span2"><h4>Type</h4></div>
  <div class="span2"><h4>Operator</h4></div>
  <div class="span3"><h4>Value</h4></div>
  <div class="span3"></div>
</div>

<div class="row">
  <% @possible_attributes.each do |val| %>
    <% group_attr_str_rel = @group_attribute_string_relationships.find_by_possible_attribute_id(val.id) %>
    <% if (group_attr_str_rel) %>
      <%= render 'existing_group_attribute_string_relationship', val: val, group_attr_str_rel: group_attr_str_rel %>
    <% else %>
      <%= render 'new_group_attribute_string_relationship', val: val %>
    <% end %>
  <% end %>
</div>

<hr />

