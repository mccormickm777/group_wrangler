<div class="row">
  <hr />
  <div class="span2">
    <%= val.name %>
  </div>
  <div class="span2">
    <%= val.attribute_type %>
  </div>
  <div class="span3">
  	<%= user_attr_str_rel.value %>
  </div>
  <% if current_user?(@user) %>
    <div class="span1">
      <%= link_to "update", edit_user_attribute_string_relationship_path(user_attr_str_rel) %>
    </div>
    <div class="span1">
      <%= link_to "clear", user_attr_str_rel, method: :delete,
                                    data: { confirm: "You sure?" } %>
    </div>
    <div class="span3"></div>
  <% else %>
    <div class="span5"></div>
  <% end %>
</div>
