<div class="row">
  <hr />
  <div class="span2">
    <%= val.name %>
  </div>
  <div class="span2">
    <%= val.attribute_type %>
  </div>
  <div class="span3">
  	<b>Not set</b>
  </div>
  <% if current_user?(@user) %>
    <div class="span1">
      <%= link_to "set", new_user_attribute_string_relationship_path(possible_attribute: val, user: @user) %>
    </div>
    <div class="span4"></div>
  <% else %>
    <div class="span5"></div>
  <% end %>
</div>
